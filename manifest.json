{
  "name": "Twitch Chat Overlay",
  "version": "0.0.1",
  "permissions": [
    "*://*.twitch.tv/"
  ],
  "manifest_version": 2,
  "content_scripts": [{
    "matches": ["*://*.twitch.tv/*"],
    "css": ["index.css"],
    "js": [
      "class_utils.js",
      "draggable.js",
      "frame_style.js",
      "index.js"
    ]
  }],
  "icons": {
    "16": "assets/logo_16x16.png",
    "48": "assets/logo_48x48.png",
    "128": "assets/logo_128x128.png"
  }
}