{
  "name": "Twitch Chat Overlay",
  "version": "0.0.1",
  "permissions": [
    "*://*.twitch.tv/"
  ],
  "manifest_version": 2,
  "content_scripts": [{
    "matches": ["*://*.twitch.tv/*"],
    "css": ["index.css"],
    "js": [
      "class_utils.js",
      "draggable.js",
      "index.js"
    ]
  }]
}